{
  "name": "@halloverden/ngx-zendesk-webwidget",
  "version": "0.2.2",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ngx-zendesk-webwidget -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "lite": "lite-server",
    "playground:build": "tsc -p playground -w",
    "playground": "concurrently \"npm run build:watch\" \"npm run playground:build\" \"npm run lite\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/halloverden/ngx-zendesk-webwidget.git"
  },
  "author": {
    "name": "Andreas Larssen",
    "email": "andreas@halloverden.no"
  },
  "keywords": [
    "angular",
    "zendesk"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/halloverden/ngx-zendesk-webwidget/issues"
  },
  "devDependencies": {
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "^3.5.0",
    "@types/node": "^12.12.6",
    "codelyzer": "^5.2.1",
    "core-js": "^3.6.1",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-rollup": "^2.16.2",
    "gulp4-run-sequence": "^1.0.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "node-sass": "^4.13.0",
    "node-sass-tilde-importer": "^1.0.2",
    "node-watch": "^0.6.3",
    "protractor": "^5.4.2",
    "rollup": "^1.27.14",
    "run-sequence": "^2.2.1",
    "rxjs": "^6.5.4",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tsickle": "^0.37.1",
    "typescript": "3.5.3",
    "zone.js": "^0.10.2"
  },
  "engines": {
    "node": "12.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
